<!DOCTYPE html>
<html>
<head>
  <title>Channelback</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>


            <div class='container'>
              <% if flash[:error] -%>
                  <p class='alert alert-error'><%=h flash[:error] %></p>
              <% end -%>
              <% if flash[:notice] -%>
                  <p class='alert alert-info'><%=h flash[:notice] %></p>
              <% end -%>
              <%= render 'layouts/header' %>

              <%= yield %>
            </div>
</body>
</html>
